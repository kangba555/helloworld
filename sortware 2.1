#include <iostream>
using namespace std;

int Max(int n, int* a)
{
	int temp = 0, record = 0, sum = 0;
	for (int i = 0; i < n; i++)
	{
		if (a[i] > 0)
			record = 1;
		if (record == 1)
		{
			temp += a[i];
			sum = sum < temp ? temp : sum;
		}
	}
	return sum;
}

int main()
{
	int n, a[999];
	cin >> n;
	while (n != EOF)
	{
		for (int i = 0; i < n; i++)
		{
			cin >> a[i];
		}
		cout << Max(n, a);
		cin >> n;
	}
}
